{
    "name": "ipimpat/vuestrap",
    "description": "A Laravel 8 package that replaces Jetstream/Jetstrap's inertia stack view files with BootstrapVue based views.",
    "authors": [
        {
            "name": "Kim Henriksen",
            "email": "kh@ipimp.at"
        }
    ],
    "require": {
        "laravel/jetstream": "^2.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "nascent-africa/jetstrap": "^2.4",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Ipimpat\\Vuestrap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ipimpat\\Vuestrap\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ipimpat\\Vuestrap\\VuestrapServiceProvider"
            ]
        }
    }
}
